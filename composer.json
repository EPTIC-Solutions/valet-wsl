    "require-dev": {
        "mockery/mockery": "~1.5.0",
        "phpunit/phpunit": "^10.2"
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "bin": [
        "valet"
